@model CrudInMvc.Models.ErrorLog
<form method="post" action="Addd"  class="mt-5">
    <div class="form-group">
        <label for="exampleInputEmail1">Id</label>
        <input type="number" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter the id" asp-for="id">
       
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">App Module</label>
        <input type="text" class="form-control" id="exampleInputPassword1" placeholder="App Module" asp-for="app_module">
    </div>
    <div class="form-group">
        <label >Error Message</label>
        <input type="text" class="form-control"  id="exampleCheck1" asp-for="error_message">
       
    </div>
    <div class="form-group">
        <label class="form-check-label" for="exampleCheck1">Date and Time</label>
        <input type="datetime" class="form-control" id="exampleCheck1" asp-for="created_date ">

    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>